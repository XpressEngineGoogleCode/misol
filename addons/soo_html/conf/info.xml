  <link>http://www.imsoo.net</link>
  <author email_address="misol221@paran.com" link="http://www.imsoo.net">
    <name xml:lang="ko">민수</name>
    <name xml:lang="en">Min-Soo Kim</name>
    <name xml:lang="zh-CN">金旻洙</name>
  </author>
  <extra_vars>
    <var name="tag_match_select" type="select">
      <title xml:lang="ko">강제 시작-끝 태그 맞춤</title>
      <options value="N">
        <title xml:lang="ko">맞추지 않음</title>
      </options>
      <options value="Y">
        <title xml:lang="ko">강제 맞춤</title>
      </options>
      <description xml:lang="ko">강제로 시작태그와 끝 태그를 맞춥니다.
        문서 형식이 변경될 수 있습니다.
      </description>
    </var>
  </extra_vars>
</addon>